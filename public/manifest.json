{
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "manifest_version": 3,
  "name": "Clear Calendar",
  "description": "A Chromium extension that lets you turn off event title in Google Calendar",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "permissions": ["scripting"],
  "host_permissions": ["https://calendar.google.com/*"]
}
